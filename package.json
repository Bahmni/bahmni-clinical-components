{
  "name": "bahmni-clinical-components",
  "version": "0.0.4",
  "description": "Repository for bahmni clinical components",
  "license": "GPL-2.0",
  "main": "./dist/bundle.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "webpack --display-modules",
    "pretest": "rimraf coverage/*",
    "test": "jest --config jest-config.json",
    "posttest": "eslint --ext .js --ext .jsx ./src ./test",
    "eslintFix": "eslint --ext .js --ext .jsx ./src ./test --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "publish": "publish || true",
    "ci": "yarn run clean && yarn run build && yarn run publish",
    "bump-up": "npm version patch -m 'Bumped to version %s'",
    "watch": "webpack --watch"
  },
  "author": "bahmni@thoughtworks.com",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "copyfiles": "^1.0.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-import-resolver-webpack": "^0.4.0",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-jsx-control-statements": "^2.1.1",
    "eslint-plugin-react": "^5.2.2",
    "fetch-mock": "^5.8.1",
    "jest": "^18.0.0",
    "jest-cli": "^18.1.0",
    "publish": "^0.6.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-test-renderer": "^15.4.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "moment": "^2.10.0",
    "react-modal": "^1.6.5",
    "bahmni-form-controls": "^0.1.0",
    "react-select": "1.0.0-rc.1",
    "webpack": "^1.14.0",
    "whatwg-fetch": "^1.0.0"
  }
}
